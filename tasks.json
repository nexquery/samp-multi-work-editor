{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-normal",
      "command": "${workspaceRoot}/pawno/pawncc.exe",
      "args": [
        "${file}",
        "-D${fileDirname}",
        "-;+",
        "-(+",
        "-d3"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": false,
      "problemMatcher": "$pawncc"
    }
  ]
}